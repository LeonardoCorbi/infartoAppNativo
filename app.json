{
  "expo": {
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "name": "Coração Alerta",
    "slug": "CoracaoAlerta",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0E446E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.leonardocorbi.coracaoalerta"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.leonardocorbi.coracaoalerta"

    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
